<merge
    xmlns:latin="http://schemas.android.com/apk/res-auto"
>
    <!-- U+3141: "ㅁ" HANGUL LETTER PIEUP
         U+0021: "!" EXCLAMATION MARK -->
    <Key
        latin:keySpec="&#x3141;"
        latin:keyHintLabel="!"
        latin:moreKeys="!"
        />

    <!-- U+3134: "ㄴ" HANGUL LETTER PIEUP
         U+0040: "@" COMMERCIAL AT -->
    <Key
        latin:keySpec="&#x3134;"
        latin:keyHintLabel="@"
        latin:moreKeys="@"
        />

    <!-- U+3147: "ㅇ" HANGUL LETTER IEUNG
         U+0023: "#" NUMBER SIGN -->
    <Key
        latin:keySpec="&#x3147;"
        latin:keyHintLabel="#"
        latin:moreKeys="#"
        />

    <!-- U+3139: "ㄹ" HANGUL LETTER RIEUL
         U+20A9: "₩" WON SIGN -->
    <Key
        latin:keySpec="&#x3139;"
        latin:keyHintLabel="&#x20A9;"
        latin:moreKeys="!text/keyspec_currency"
        />

    <!-- U+314E: "ㅎ" HANGUL LETTER HIEUH
         U+0025: "%" PERCENT SIGN -->
    <Key
        latin:keySpec="&#x314E;"
        latin:keyHintLabel="!text/keyspec_symbols_percent"
        latin:moreKeys="!text/keyspec_symbols_percent"
        />

    <!-- U+3157: "ㅗ" HANGUL LETTER O
         U+0026: "&" AMPERSAND -->
    <Key
        latin:keySpec="&#x3157;"
        latin:keyHintLabel="&#x0026;"
        latin:moreKeys="&#x0026;"
        />

    <!-- U+3153: "ㅓ" HANGUL LETTER EO
         U+002A: "*" ASTERISK -->
    <Key
        latin:keySpec="&#x3153;"
        latin:keyHintLabel="&#x002A;"
        latin:moreKeys="&#x002A;"
        />

    <!-- U+314F: "ㅏ" HANGUL LETTER A
         U+0028: "(" LEFT PARENTHESIS -->
    <Key
        latin:keySpec="&#x314F;"
        latin:keyHintLabel="&#x0028;"
        latin:moreKeys="&#x0028;"
        />

    <!-- U+3163: "ㅣ" HANGUL LETTER I
         U+0029: ")" RIGHT PARENTHESIS-->
    <Key
        latin:keySpec="&#x3163;"
        latin:keyHintLabel="&#x0029;"
        latin:moreKeys="&#x0029;"
        />
</merge>
