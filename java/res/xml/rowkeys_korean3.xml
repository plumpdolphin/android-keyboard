<merge
    xmlns:latin="http://schemas.android.com/apk/res-auto"
>
    <!-- U+314B: "ㅋ" HANGUL LETTER KHIEUKH
         U+002D: "-" HYPHEN-MINUS -->
    <Key
        latin:keySpec="&#x314B;"
        latin:keyHintLabel="-"
        latin:moreKeys="-"
        />

    <!-- U+314C: "ㅌ" HANGUL LETTER THIEUTH
         U+0027: "'" APOSTROPHE -->
    <Key
        latin:keySpec="&#x314C;"
        latin:keyHintLabel="'"
        latin:moreKeys="'"
        />

    <!-- U+314A: "ㅊ" HANGUL LETTER CHIEUCH
         U+0022: """ QUOTATION MARK -->
    <Key
        latin:keySpec="&#x314A;"
        latin:keyHintLabel="&#x0022;"
        latin:moreKeys="&#x0022;"
        />

    <!-- U+314D: "ㅍ" HANGUL LETTER PHIEUPH
         U+003A: ":" COLON -->
    <Key
        latin:keySpec="&#x314D;"
        latin:keyHintLabel="&#x003A;"
        latin:moreKeys="&#x003A;"
        />

    <!-- U+3160: "ㅠ" HANGUL LETTER YU
         U+003B: ";" SEMICOLON -->
    <Key
        latin:keySpec="&#x3160;"
        latin:keyHintLabel="&#x003B;"
        latin:moreKeys="&#x003B;"
        />

    <!-- U+315C: "ㅜ" HANGUL LETTER U
         U+002C: "," COMMA -->
    <Key
        latin:keySpec="&#x315C;"
        latin:keyHintLabel="!text/keyhintlabel_tablet_comma"
        latin:moreKeys="!text/morekeys_tablet_comma"
        />

    <!-- U+3161: "ㅡ" HANGUL LETTER EU
         U+003F: "?" QUESTION MARK -->
    <Key
        latin:keySpec="&#x3161;"
        latin:keyHintLabel="&#x003F;"
        latin:moreKeys="&#x003F;"
        />
</merge>
